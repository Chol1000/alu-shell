#!/usr/bin/env bash
# This script parses Apache log files using awk

# Define the function to parse the log file
parse_log() {
    awk '{ print $1, $9 }' "$1"
}

# Check if the file exists and is not empty
if [ -s "$1" ]; then
    # Parse the log file if it exists and is not empty
    parse_log "$1"
else
    # Print a message if the file is empty or does not exist
    echo "The log file is either empty or does not exist."
fi
